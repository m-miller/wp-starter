{
  "name": "webdeveric/wp-starter",
  "description": "WordPress dependency management with Composer",
  "license": "proprietary",
  "keywords": [ "WordPress", "composer", "dependency" ],
  "authors": [
    {
      "name": "Eric King",
      "email": "eric@webdeveric.com"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/webdeveric/http-api-debug.git"
    },
    {
      "type": "git",
      "url": "https://github.com/lplabs/wp-composer-autoloader.git"
    },
    {
      "type": "git",
      "url": "https://github.com/lplabs/wordpress-muplugin-installer.git"
    },
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "webdeveric\\WPStarter\\": "src/"
    }
  },
  "require": {
    "php": ">=5.5",
    "johnpbloch/wordpress": "^4.5.0",
    "lplabs/wp-composer-autoloader": "^0.1.0",
    "lplabs/wordpress-muplugin-installer": "^1.0.0",
    "wpackagist-plugin/rest-api": "^2.0-beta13",
    "wpackagist-plugin/what-template-am-i-using": "^0.2.0",
    "wpackagist-plugin/recently-edited-content-widget": "^0.3.2",
    "wpackagist-plugin/get-post-content-shortcode": "^0.3.2"
  },
  "require-dev": {
    "webdeveric/http-api-debug": "dev-master"
  },
  "extra": {
    "wordpress-install-dir": "cms"
  },
  "scripts": {
    "post-update-cmd": "webdeveric\\WPStarter\\Setup::getInstance",
    "post-install-cmd": "webdeveric\\WPStarter\\Setup::getInstance",
    "post-create-project-cmd": "webdeveric\\WPStarter\\Setup::getInstance"
  }
}
